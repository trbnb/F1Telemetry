<fluent:RibbonWindow x:Class="DesktopClient.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopClient.Main"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:sectortimes="clr-namespace:DesktopClient.UserControls.SectorTimes"
        xmlns:fluent="urn:fluent-ribbon"
        xmlns:userControls="clr-namespace:DesktopClient.UserControls"
        xmlns:converters="clr-namespace:DesktopClient.Converters"
        mc:Ignorable="d"
        Title="MainWindow" WindowState="Normal" Height="600" Width="800">
    
    <Window.DataContext>
        <local:MainViewModel />
    </Window.DataContext>
    
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <converters:IntToStringConverter x:Key="IntToStringConverter"/>
    </Window.Resources>
    
    <DockPanel LastChildFill="True">
        <fluent:Ribbon DockPanel.Dock="Top">

            <fluent:RibbonTabItem Header="Main">
                <fluent:RibbonGroupBox Header="Source">
                    <fluent:ToggleButton Command="{Binding SetToLiveSessionCommand}" IsChecked="{Binding IsLiveSession, Mode=OneWay}" Header="Live" Size="Large" LargeIcon="{StaticResource Record32}" />
                    <fluent:ToggleButton Command="{Binding SetToFileSessionCommand}" IsChecked="{Binding IsFileSession, Mode=OneWay}" Header="File" Size="Large" LargeIcon="{StaticResource Open32}"/>
                </fluent:RibbonGroupBox>
                <fluent:RibbonGroupBox Header="Control">
                    <fluent:Button Header="Start" Command="{Binding StartCommand}" Size="Large" LargeIcon="{StaticResource Play32}" />
                    <fluent:Button Header="Pause" Command="{Binding PauseCommand}" Size="Large" LargeIcon="{StaticResource Pause32}" />
                </fluent:RibbonGroupBox>
                <fluent:RibbonGroupBox Header="Settings">
                    <fluent:Button Header="Settings" Size="Large" LargeIcon="{StaticResource Settings32}" />
                </fluent:RibbonGroupBox>
            </fluent:RibbonTabItem>
        </fluent:Ribbon>
        
        <StatusBar Background="DarkRed" DockPanel.Dock="Bottom">
            <StatusBarItem HorizontalAlignment="Right" >
                <TextBlock Foreground="White" Padding="8,0,8,0" Text="for F1 2017"/>
            </StatusBarItem>
        </StatusBar>

        <StackPanel Margin="8" Orientation="Horizontal">
            <StackPanel>
                <Grid Width="200" Height="200">
                    <lvc:Gauge InnerRadius="80" Grid.Row="0" Grid.Column="0"
                               From="0" To="370" Value="{Binding Speed, FallbackValue=215}"
                               HighFontSize="50" Foreground="#424242" GaugeActiveFill="DarkGreen" 
                               Stroke="LightSlateGray" StrokeThickness="1.2" GaugeBackground="Transparent" />

                    <TextBlock Grid.Row="0" Grid.Column="0" Background="#00000000" Text="KM/H" HorizontalAlignment="Center" Margin="0, 140, 0, 0" FontSize="18"/>
                </Grid>

                <TextBlock Margin="0,-16,0,16" HorizontalAlignment="Center">
                    <Run FontSize="28" Text="{Binding EngineRpm, Mode=OneWay, Converter={StaticResource IntToStringConverter}}" />
                    <Run FontSize="16" FontWeight="DemiBold" Text=" RPM"/>
                </TextBlock>

                <userControls:PedalsControl Padding="8,0,8,0" Pedals="{Binding Pedals}"/>

                <sectortimes:SectorTimes HorizontalAlignment="Center" 
                                         Margin="0,16,0,0"
                                         Sector1="{Binding Sector1Time}" 
                                         Sector2="{Binding Sector2Time}"
                                         Sector3="{Binding Sector3Time}"
                                         Total="{Binding TotalLaptime}"/>
        
            </StackPanel>
            
            <ListView BorderThickness="0" Width="300" ItemsSource="{Binding DriversInPositions}">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="40" />
                                <ColumnDefinition Width="150" />
                                <ColumnDefinition Width="50" />
                            </Grid.ColumnDefinitions>
                            <TextBlock HorizontalAlignment="Right" FontSize="18" Padding="0,0,8,0" Grid.Column="0" Text="{Binding LatestLiveData.CarPosition}" />
                            <TextBlock Grid.Column="1" FontSize="18" Text="{Binding DriverName}" />
                            <TextBlock HorizontalAlignment="Center" FontSize="18" Grid.Column="2" Text="PIT" Foreground="Red" Visibility="{Binding IsInPits, Converter={StaticResource BooleanToVisibilityConverter}}" />
                        </Grid>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackPanel>
    </DockPanel>
